{
  "parser": "babel-eslint",
  "plugins": ["react"],
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "react/prefer-stateless-function": "off"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["client"]
      }
    }
  }
}
